<script>

    var dialog = new top.TideDialog();  //弹窗公共变量  
    function fieldconfirm() {
        var checkedRadio = $('li input:radio:checked');
        if (checkedRadio.length == 0) {
            dialog.showAlert("请选择图标！", "danger");
            return false;
        } else {
            var tr = checkedRadio.parents("li");
            var _class = tr.find(".fa").attr("class");
            console.log(_class)
            var icon = '<i class="' + _class + ' mg-r-5"></i>';
            top.TideDialogClose({suffix: '_3', recall: true, returnValue: {icon: icon}});
        }
    }
</script>
